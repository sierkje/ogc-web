{
  "name": "ogc-web-app",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "emotion"
    ],
    "rules": {
      "emotion/jsx-import": "error"
    }
  },
  "browserslist": {
